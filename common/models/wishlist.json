{
  "name": "wishlist",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "CreatedAt": {
      "type": "date",
      "defaultFn": "now"
    },
    "LastUpdated": {
      "type": "date"
    },
    "UpdatedBy": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "product": {
      "type": "belongsTo",
      "model": "product",
      "foreignKey": "productId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
